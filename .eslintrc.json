{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": ["plugin:@typescript-eslint/recommended", "eslint:all"],
  "env": {
    "node": true,
    "es6": true,
    "es2020": true,
    "jest": true
  },
  "globals": { "fetch": true },
  "rules": {
    "@typescript-eslint/no-explicit-any": 0,
    "@typescript-eslint/explicit-function-return-type": 0,
    "@typescript-eslint/interface-name-prefix": 0,
    "@typescript-eslint/no-var-requires": 0,
    "@typescript-eslint/no-this-alias": 0,
    "one-var": "off",
    "id-length": "off",
    "implicit-arrow-linebreak": "off",
    "multiline-ternary": "off",
    "no-ternary": "off",
    "no-negated-condition": "off",
    "no-confusing-arrow": "off",
    "no-undefined": "off",
    "padded-blocks": "off",
    "function-paren-newline": "off",
    "nonblock-statement-body-position": "off",
    "sort-keys": "off",
    "function-call-argument-newline": "off",
    "max-statements": "off",
    "object-property-newline": "off",
    "no-magic-numbers": "off",
    "max-lines-per-function": "off",
    "spaced-comment": "off",
    "capitalized-comments": "off",
    "no-console": "off",
    "max-params": "off",
    "callback-return": "off",
    "no-useless-constructor":"off",
    "lines-around-comment":"off",
    "multiline-comment-style":"off",
    "no-inline-comments":"off",
    "line-comment-position":"off",
    "global-require":"off",
    "prefer-exponentiation-operator":"off",
    "consistent-this":"off",
    "max-classes-per-file":"off",
    "no-mixed-operators":"off",
    "new-cap":"off",
    "sort-imports":"off",
    "no-nested-ternary":"off",
    "require-await":"off",
    "no-process-env":"off",
    "max-lines":"warn",
    "no-warning-comments":"warn",
    "no-empty-function": ["error", { "allow": ["constructors"] }],
    "no-underscore-dangle": [
      "off",
      { "allowAfterThis": true, "allowAfterSuper": true }
    ],
    "no-multiple-empty-lines": "warn",
    "semi": "warn",
    "object-curly-spacing": ["warn", "always"],
    "no-unused-vars": "warn",
    "max-len": ["warn", { "code": 100, "tabWidth": 2 }],
    "indent": ["warn", 2],
    "quotes": ["warn", "double"],
    "comma-spacing": "warn",
    "no-trailing-spaces": "warn",
    "no-multi-spaces": "warn",
    "arrow-spacing": "warn",
    "space-infix-ops": "warn",
    "semi-spacing": "warn",
    "curly": ["warn", "multi"],
    "arrow-parens": ["warn", "as-needed"],
    "quote-props": ["warn", "as-needed"],
    "prefer-template": "warn",
    "array-bracket-newline": ["warn", { "multiline": true }],
    "array-element-newline": ["warn", "consistent"],
    "@typescript-eslint/no-use-before-define": ["warn", { "functions": true, "classes": false }] ,
    "newline-per-chained-call":"off",
    "no-extra-parens": "off",
    "@typescript-eslint/no-extra-parens": ["error"], "key-spacing":"warn",
      "dot-location": ["warn", "property"]
  }
}
