{
  "$schema": "./schema.json",
  "$types": {
    "$customType": {
      "$keyOf": [
        "$types"
      ]
    },
    "$optionalPositiveInteger": [
      "?",
      {
        "$number": {
          "min": 0,
          "integer": true
        }
      }
    ],
    "$simpleTypes": {
      "$enum": [
        "string",
        "boolean",
        "number",
        "integer",
        "null",
        "?",
        "any"
      ]
    },
    "$enumType": {
      "name": [
        "?",
        "string"
      ],
      "description": [
        "?",
        "string"
      ],
      "\\$enum": {
        "minLength": 1,
        "$array": "string"
      }
    }
  },
  "$type": {
    "oneKey": "$customType",
    "\\$types": {
      "a": "string",
      "b": "number"
    },
    "bigPP": {
      "$propertyPath": {}
    }
  }
}